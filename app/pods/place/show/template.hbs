<div class="row">
  <div class="container">
    <div class="col col-sm-6 col-md-4 col-lg-4">
      <h4>{{model.name}}</h4>
      <p>gdzie: {{model.location}}</p>
      <p>co: {{model.description}}</p>
      {{#link-to 'place.edit' model}}
        <button class="button button-info">edit</button>
      {{/link-to}}
    </div>
    <div class="col col-sm-6 col-md-4 col-lg-4">
      {{location-map  markers=markers
                      centerLat=model.lat
                      centerLng=model.lng}}
    </div>
  </div>
</div>
